    "migrations:delete": "sequelize-cli db:migrate:undo:all"
  },
  "bugs": {
    "url": "https://github.com/robertotorres-dev/siiges-services/issues"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.0",
